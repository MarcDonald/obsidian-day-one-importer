{
	"name": "obsidian-day-one-importer",
	"version": "0.1.0",
	"description": "Allows you to import Day One JSON into Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"lint": "eslint -c .eslintrc . && prettier -c .",
		"test": "TZ=UTC jest",
		"test:watch": "TZ=UTC jest --watch",
		"lint:fix": "eslint -c .eslintrc --fix . && prettier -c --write .",
		"typecheck": "tsc -noEmit -skipLibCheck",
		"build": "npm run typecheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"prepare": "husky"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin"
	],
	"author": "Marc Donald",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/marcdonald/obsidian-day-one-importer.git"
	},
	"devDependencies": {
		"@types/node": "^18.19.31",
		"eslint": "^8.57.0",
		"@typescript-eslint/eslint-plugin": "7.7.0",
		"@typescript-eslint/parser": "7.7.0",
		"globals": "^15.0.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"husky": "^9.0.11",
		"ts-jest": "^29.1.2",
		"jest": "^29.7.0",
		"@jest/globals": "^29.7.0",
		"@types/jest": "^29.5.12",
		"obsidian": "^1.5.7-1",
		"prettier": "^3.2.5",
		"tslib": "2.4.0",
		"lint-staged": "^15.2.2",
		"typescript": "4.7.4"
	}
}
