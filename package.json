{
	"name": "obsidian-day-one-importer",
	"version": "0.0.0",
	"description": "Allows you to import Day One JSON into Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"lint": "eslint -c .eslintrc . && prettier -c .",
		"lint:fix": "eslint -c .eslintrc --fix . && prettier -c --write .",
		"typecheck": "tsc -noEmit -skipLibCheck",
		"build": "npm run typecheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"prepare": "husky"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"eslint": "^8.57.0",
		"@typescript-eslint/eslint-plugin": "7.7.0",
		"@typescript-eslint/parser": "7.7.0",
		"globals": "^15.0.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"husky": "^9.0.11",
		"obsidian": "latest",
		"prettier": "^3.2.5",
		"tslib": "2.4.0",
		"lint-staged": "^15.2.2",
		"typescript": "4.7.4"
	}
}
